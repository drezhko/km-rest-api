        "priority" : {
          "type" : "integer",
          "format" : "int32",
          "minimum" : -1,
          "maximum" : 12,
          "example" : 0
        },
        "previousPriority" : {
          "type" : "integer",
          "format" : "int32",
          "minimum" : -1,
          "maximum" : 12,
          "example" : 0
